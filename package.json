{
  "name": "portfolio-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "get-deps": "npm install --legacy-peer-deps",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "serve -s dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev-app": "npm run dev",
    "prod-app": "npm run build && npm run serve",
    "prod-app-formal": "pkill -f nginx & npm run build && rm -R /usr/local/etc/nginx/portfolio-dist && cp -R dist /usr/local/etc/nginx/portfolio-dist && nginx"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@leecheuk/react-google-login": "^5.4.1",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-charts": "^7.23.2",
    "@mui/x-date-pickers": "^6.20.2",
    "axios": "^1.7.7",
    "currency.js": "^2.0.4",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-calendar": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "react-time-picker": "^6.6.0",
    "sass": "^1.66.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "why-is-node-running": "^3.2.0"
  }
}
